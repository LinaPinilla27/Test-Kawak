{"ast":null,"code":"import { defineComponent } from \"vue\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport FullCalendar from \"@fullcalendar/vue3\";\nimport dayGridPlugin from \"@fullcalendar/daygrid\";\nimport timeGridPlugin from \"@fullcalendar/timegrid\";\nimport interactionPlugin from \"@fullcalendar/interaction\";\nimport { createEventId } from \"./event-utils\";\nexport default defineComponent({\n  components: {\n    FullCalendar\n  },\n  computed: {\n    ...mapGetters([\"getAllEvents\"])\n  },\n  data() {\n    return {\n      calendarOptions: {\n        // ... (tu configuración existente)\n        initialEvents: this.getAllEvents\n      }\n    };\n  },\n  methods: {\n    ...mapActions([\"addEvent\", \"removeEvent\"]),\n    // Resto de tus métodos existentes\n    handleDateSelect(selectInfo) {\n      let title = prompt(\"Please enter a new title for your event\");\n      if (title) {\n        const event = {\n          id: createEventId(),\n          title,\n          start: selectInfo.startStr,\n          end: selectInfo.endStr,\n          allDay: selectInfo.allDay\n        };\n        this.addEvent(event);\n      }\n    },\n    handleEventClick(clickInfo) {\n      if (confirm(`Are you sure you want to delete the event '${clickInfo.event.title}'`)) {\n        this.removeEvent(clickInfo.event.id);\n      }\n    },\n    handleEvents(events) {\n      // No necesitas gestionar los eventos aquí si usas Vuex\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","mapActions","mapGetters","FullCalendar","dayGridPlugin","timeGridPlugin","interactionPlugin","createEventId","components","computed","data","calendarOptions","initialEvents","getAllEvents","methods","handleDateSelect","selectInfo","title","prompt","event","id","start","startStr","end","endStr","allDay","addEvent","handleEventClick","clickInfo","confirm","removeEvent","handleEvents","events"],"sources":["/Users/lina/Desktop/vue/notes-calendar/src/components/Calendario.vue"],"sourcesContent":["<script>\nimport { defineComponent } from \"vue\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport FullCalendar from \"@fullcalendar/vue3\";\nimport dayGridPlugin from \"@fullcalendar/daygrid\";\nimport timeGridPlugin from \"@fullcalendar/timegrid\";\nimport interactionPlugin from \"@fullcalendar/interaction\";\nimport { createEventId } from \"./event-utils\";\n\nexport default defineComponent({\n  components: {\n    FullCalendar,\n  },\n  computed: {\n    ...mapGetters([\"getAllEvents\"]),\n  },\n  data() {\n    return {\n      calendarOptions: {\n        // ... (tu configuración existente)\n        initialEvents: this.getAllEvents,\n      },\n    };\n  },\n  methods: {\n    ...mapActions([\"addEvent\", \"removeEvent\"]),\n    // Resto de tus métodos existentes\n    handleDateSelect(selectInfo) {\n      let title = prompt(\"Please enter a new title for your event\");\n\n      if (title) {\n        const event = {\n          id: createEventId(),\n          title,\n          start: selectInfo.startStr,\n          end: selectInfo.endStr,\n          allDay: selectInfo.allDay,\n        };\n\n        this.addEvent(event);\n      }\n    },\n    handleEventClick(clickInfo) {\n      if (\n        confirm(\n          `Are you sure you want to delete the event '${clickInfo.event.title}'`\n        )\n      ) {\n        this.removeEvent(clickInfo.event.id);\n      }\n    },\n    handleEvents(events) {\n      // No necesitas gestionar los eventos aquí si usas Vuex\n    },\n  },\n});\n</script>\n\n\n<template>\n  <div class='demo-app'>\n    <div class='demo-app-main'>\n      <FullCalendar\n        class='demo-app-calendar'\n        :options='calendarOptions'\n      >\n        <template v-slot:eventContent='arg'>\n          <b>{{ arg.timeText }}</b>\n          <i>{{ arg.event.title }}</i>\n        </template>\n      </FullCalendar>\n    </div>\n  </div>\n</template>\n\n<style lang='css'>\n\nh2 {\n  margin: 0;\n  font-size: 16px;\n}\n\nul {\n  margin: 0;\n  padding: 0 0 0 1.5em;\n}\n\nli {\n  margin: 1.5em 0;\n  padding: 0;\n}\n\nb { /* used for event dates/times */\n  margin-right: 3px;\n}\n\n.demo-app {\n  display: flex;\n  min-height: 100%;\n  font-family: Arial, Helvetica Neue, Helvetica, sans-serif;\n  font-size: 14px;\n}\n\n.demo-app-sidebar {\n  width: 300px;\n  line-height: 1.5;\n  background: #eaf9ff;\n  border-right: 1px solid #d3e2e8;\n}\n\n.demo-app-sidebar-section {\n  padding: 2em;\n}\n\n.demo-app-main {\n  flex-grow: 1;\n  padding: 3em;\n}\n\n.fc { /* the calendar root */\n  max-width: 1100px;\n  margin: 0 auto;\n}\n\n</style>\n"],"mappings":"AACA,SAASA,eAAc,QAAS,KAAK;AACrC,SAASC,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,aAAY,MAAO,uBAAuB;AACjD,OAAOC,cAAa,MAAO,wBAAwB;AACnD,OAAOC,iBAAgB,MAAO,2BAA2B;AACzD,SAASC,aAAY,QAAS,eAAe;AAE7C,eAAeP,eAAe,CAAC;EAC7BQ,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,QAAQ,EAAE;IACR,GAAGP,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE;QACf;QACAC,aAAa,EAAE,IAAI,CAACC;MACtB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGb,UAAU,CAAC,CAAC,UAAU,EAAE,aAAa,CAAC,CAAC;IAC1C;IACAc,gBAAgBA,CAACC,UAAU,EAAE;MAC3B,IAAIC,KAAI,GAAIC,MAAM,CAAC,yCAAyC,CAAC;MAE7D,IAAID,KAAK,EAAE;QACT,MAAME,KAAI,GAAI;UACZC,EAAE,EAAEb,aAAa,CAAC,CAAC;UACnBU,KAAK;UACLI,KAAK,EAAEL,UAAU,CAACM,QAAQ;UAC1BC,GAAG,EAAEP,UAAU,CAACQ,MAAM;UACtBC,MAAM,EAAET,UAAU,CAACS;QACrB,CAAC;QAED,IAAI,CAACC,QAAQ,CAACP,KAAK,CAAC;MACtB;IACF,CAAC;IACDQ,gBAAgBA,CAACC,SAAS,EAAE;MAC1B,IACEC,OAAO,CACJ,8CAA6CD,SAAS,CAACT,KAAK,CAACF,KAAM,GACtE,GACA;QACA,IAAI,CAACa,WAAW,CAACF,SAAS,CAACT,KAAK,CAACC,EAAE,CAAC;MACtC;IACF,CAAC;IACDW,YAAYA,CAACC,MAAM,EAAE;MACnB;IAAA;EAEJ;AACF,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}