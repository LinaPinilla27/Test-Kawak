{"ast":null,"code":"import Fullcalendar from '@fullcalendar/vue3';\nimport DayGridPlugin from '@fullcalendar/daygrid';\nimport TimeGridPlugin from '@fullcalendar/timegrid';\nimport InteractionPlugin from '@fullcalendar/interaction';\nimport ListPlugin from '@fullcalendar/list';\nimport { mapGetters } from 'vuex';\nexport default {\n  data: () => ({\n    calendarPlugins: [DayGridPlugin, TimeGridPlugin, InteractionPlugin, ListPlugin]\n  }),\n  components: {\n    Fullcalendar\n  },\n  computed: {\n    ...mapGetters([\"EVENTS\"])\n  },\n  methods: {\n    newEvent() {\n      this.$modal.show(\"modal-example\");\n    },\n    renderEvent(arg) {\n      let span = document.createElement('span');\n      span.setAttribute('class', 'fa fa-comments');\n      arg.el.appendChild(span);\n      span.addEventListener('click', event => {\n        event.stopPropagation();\n        this.$modal.show(\"comments-modal\");\n      });\n    },\n    updateEvent(arg) {\n      this.$store.commit(\"UPDATE_EVENT\", arg.event);\n    },\n    handleSelect(arg) {\n      this.$store.commit(\"ADD_EVENT\", {\n        id: new Date().getTime(),\n        title: \"Something\",\n        start: arg.start,\n        end: arg.end,\n        allDay: arg.allDay\n      });\n    },\n    handleEventClick(arg) {\n      this.$modal.show(\"event-modal\", {\n        event: arg.event\n      });\n    },\n    beforeOpen(event, modal) {\n      // Puedes realizar alguna l贸gica antes de abrir el modal\n      console.log(`Abriendo el modal: ${modal.name}`);\n    },\n    onModalClose(event, modal) {\n      // Puedes realizar alguna l贸gica cuando se cierra el modal\n      console.log(`Cerrando el modal: ${modal.name}`);\n    }\n  }\n};","map":{"version":3,"names":["Fullcalendar","DayGridPlugin","TimeGridPlugin","InteractionPlugin","ListPlugin","mapGetters","data","calendarPlugins","components","computed","methods","newEvent","$modal","show","renderEvent","arg","span","document","createElement","setAttribute","el","appendChild","addEventListener","event","stopPropagation","updateEvent","$store","commit","handleSelect","id","Date","getTime","title","start","end","allDay","handleEventClick","beforeOpen","modal","console","log","name","onModalClose"],"sources":["/Users/lina/Desktop/vue/notes-calendar/src/components/Calendario.vue"],"sourcesContent":["<template>\n  <div>\n    <button @click=\"newEvent\">New event</button>\n    <Fullcalendar \n        :plugins=\"calendarPlugins\" \n        :header=\"{\n            left: 'title',\n            center: 'dayGridMonth, timeGridWeek, timeGridDay, listWeek',\n            right: 'prev today next'\n        }\"\n        :buttonText=\"{\n            today: 'Aujourd\\'hui',\n            month: 'Mois',\n            week: 'Semaine',\n            day: 'Jour',\n            list: 'Liste'\n        }\"\n        :weekends=\"false\"\n        :selectable=\"true\"\n        :editable=\"true\"\n        :events=\"EVENTS\"\n        @select=\"handleSelect\"\n        @eventClick=\"handleEventClick\"\n        @eventResize=\"updateEvent\"\n        @eventDrop=\"updateEvent\"\n        @eventRender=\"renderEvent\"\n    />\n\n    <!-- Usar modales de vue-js-modal -->\n    <modal name=\"modal-example\" @before-open=\"beforeOpen\" @close=\"onModalClose\">\n      <h2>Modal Example</h2>\n      <p>This is the content of the modal.</p>\n    </modal>\n  </div>\n</template>\n\n<script>\n\nimport Fullcalendar from '@fullcalendar/vue3';\nimport DayGridPlugin from '@fullcalendar/daygrid';\nimport TimeGridPlugin from '@fullcalendar/timegrid';\nimport InteractionPlugin from '@fullcalendar/interaction';\nimport ListPlugin from '@fullcalendar/list';\nimport { mapGetters } from 'vuex';\n\nexport default {\n    data: () => ({\n        calendarPlugins: [\n            DayGridPlugin,\n            TimeGridPlugin,\n            InteractionPlugin,\n            ListPlugin\n        ]\n    }),\n    components: { Fullcalendar },\n    computed: {\n        ...mapGetters([\"EVENTS\"])\n    },\n    methods: {\n        newEvent() {\n            this.$modal.show(\"modal-example\");\n        },\n        renderEvent(arg) {\n            let span = document.createElement('span');\n            span.setAttribute('class', 'fa fa-comments');\n            arg.el.appendChild(span);\n            span.addEventListener('click', event => {\n                event.stopPropagation(); \n                this.$modal.show(\"comments-modal\");\n            });\n        },\n        updateEvent(arg) {\n            this.$store.commit(\"UPDATE_EVENT\", arg.event);\n        },\n        handleSelect(arg) {\n            this.$store.commit(\"ADD_EVENT\", {\n                id: (new Date()).getTime(),\n                title: \"Something\",\n                start: arg.start,\n                end: arg.end,\n                allDay: arg.allDay\n            });\n        },\n        handleEventClick(arg) {\n            this.$modal.show(\"event-modal\", { event: arg.event });\n        },\n        beforeOpen(event, modal) {\n            // Puedes realizar alguna l贸gica antes de abrir el modal\n            console.log(`Abriendo el modal: ${modal.name}`);\n        },\n        onModalClose(event, modal) {\n            // Puedes realizar alguna l贸gica cuando se cierra el modal\n            console.log(`Cerrando el modal: ${modal.name}`);\n        }\n    }\n};\n</script>\n\n<style>\n/* Estilos adicionales si es necesario */\n</style>\n"],"mappings":"AAsCA,OAAOA,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,aAAY,MAAO,uBAAuB;AACjD,OAAOC,cAAa,MAAO,wBAAwB;AACnD,OAAOC,iBAAgB,MAAO,2BAA2B;AACzD,OAAOC,UAAS,MAAO,oBAAoB;AAC3C,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACXC,IAAI,EAAEA,CAAA,MAAO;IACTC,eAAe,EAAE,CACbN,aAAa,EACbC,cAAc,EACdC,iBAAiB,EACjBC,UAAS;EAEjB,CAAC,CAAC;EACFI,UAAU,EAAE;IAAER;EAAa,CAAC;EAC5BS,QAAQ,EAAE;IACN,GAAGJ,UAAU,CAAC,CAAC,QAAQ,CAAC;EAC5B,CAAC;EACDK,OAAO,EAAE;IACLC,QAAQA,CAAA,EAAG;MACP,IAAI,CAACC,MAAM,CAACC,IAAI,CAAC,eAAe,CAAC;IACrC,CAAC;IACDC,WAAWA,CAACC,GAAG,EAAE;MACb,IAAIC,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,MAAM,CAAC;MACzCF,IAAI,CAACG