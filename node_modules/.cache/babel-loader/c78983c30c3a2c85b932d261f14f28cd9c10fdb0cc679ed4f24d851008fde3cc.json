{"ast":null,"code":"import { defineComponent } from 'vue';\nimport FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport timeGridPlugin from '@fullcalendar/timegrid';\nimport interactionPlugin from '@fullcalendar/interaction';\nimport { createEventId } from './event-utils';\nimport { mapGetters, mapActions } from 'vuex';\nexport default defineComponent({\n  components: {\n    FullCalendar\n  },\n  computed: {\n    ...mapGetters(['events']) // Actualiza aquí el nombre del getter\n  },\n  methods: {\n    ...mapActions(['addEvent', 'removeEvent']),\n    handleDateSelect(selectInfo) {\n      const title = prompt('Adicionar nota');\n      if (title) {\n        const event = {\n          id: createEventId(),\n          title,\n          start: selectInfo.startStr,\n          end: selectInfo.endStr,\n          allDay: selectInfo.allDay\n        };\n        this.addEvent(event);\n      }\n    },\n    handleEventClick(clickInfo) {\n      if (confirm(`Quieres eliminar la nota '${clickInfo.event.title}'`)) {\n        this.removeEvent(clickInfo.event.id);\n      }\n    }\n  },\n  watch: {\n    // Observa cambios en la propiedad events y actualiza calendarOptions.events\n    events: {\n      handler(events) {\n        this.calendarOptions.events = events;\n      },\n      immediate: true // Ejecuta el handler inmediatamente al cargar el componente\n    }\n  },\n  data() {\n    return {\n      calendarOptions: {\n        plugins: [dayGridPlugin, timeGridPlugin, interactionPlugin],\n        headerToolbar: {\n          left: 'prev,next today',\n          center: 'title',\n          right: 'dayGridMonth,timeGridWeek,timeGridDay'\n        },\n        initialView: 'dayGridMonth',\n        events: this.events,\n        // Inicializa con los eventos actuales\n        editable: true,\n        selectable: true,\n        selectMirror: true,\n        dayMaxEvents: true,\n        weekends: true,\n        select: this.handleDateSelect,\n        eventClick: this.handleEventClick\n      }\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","FullCalendar","dayGridPlugin","timeGridPlugin","interactionPlugin","createEventId","mapGetters","mapActions","components","computed","methods","handleDateSelect","selectInfo","title","prompt","event","id","start","startStr","end","endStr","allDay","addEvent","handleEventClick","clickInfo","confirm","removeEvent","watch","events","handler","calendarOptions","immediate","data","plugins","headerToolbar","left","center","right","initialView","editable","selectable","selectMirror","dayMaxEvents","weekends","select","eventClick"],"sources":["/Users/lina/Desktop/vue/notes-calendar/src/components/Calendario.vue"],"sourcesContent":["<!-- TuComponente.vue -->\n<template>\n  <div class=\"demo-app\">\n    <div class=\"demo-app-main\">\n      <FullCalendar\n        class=\"demo-app-calendar\"\n        :options=\"calendarOptions\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\nimport FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport timeGridPlugin from '@fullcalendar/timegrid';\nimport interactionPlugin from '@fullcalendar/interaction';\nimport { createEventId } from './event-utils';\nimport { mapGetters, mapActions } from 'vuex';\n\nexport default defineComponent({\n  components: {\n    FullCalendar,\n  },\n  computed: {\n    ...mapGetters(['events']), // Actualiza aquí el nombre del getter\n  },\n  methods: {\n    ...mapActions(['addEvent', 'removeEvent']),\n    handleDateSelect(selectInfo) {\n      const title = prompt('Adicionar nota');\n      if (title) {\n        const event = {\n          id: createEventId(),\n          title,\n          start: selectInfo.startStr,\n          end: selectInfo.endStr,\n          allDay: selectInfo.allDay,\n        };\n        this.addEvent(event);\n      }\n    },\n    handleEventClick(clickInfo) {\n      if (confirm(`Quieres eliminar la nota '${clickInfo.event.title}'`)) {\n        this.removeEvent(clickInfo.event.id);\n      }\n    },\n  },\n  watch: {\n    // Observa cambios en la propiedad events y actualiza calendarOptions.events\n    events: {\n      handler(events) {\n        this.calendarOptions.events = events;\n      },\n      immediate: true, // Ejecuta el handler inmediatamente al cargar el componente\n    },\n  },\n  data() {\n    return {\n      calendarOptions: {\n        plugins: [\n          dayGridPlugin,\n          timeGridPlugin,\n          interactionPlugin,\n        ],\n        headerToolbar: {\n          left: 'prev,next today',\n          center: 'title',\n          right: 'dayGridMonth,timeGridWeek,timeGridDay',\n        },\n        initialView: 'dayGridMonth',\n        events: this.events, // Inicializa con los eventos actuales\n        editable: true,\n        selectable: true,\n        selectMirror: true,\n        dayMaxEvents: true,\n        weekends: true,\n        select: this.handleDateSelect,\n        eventClick: this.handleEventClick,\n      },\n    };\n  },\n});\n</script>\n"],"mappings":"AAaA,SAASA,eAAc,QAAS,KAAK;AACrC,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,aAAY,MAAO,uBAAuB;AACjD,OAAOC,cAAa,MAAO,wBAAwB;AACnD,OAAOC,iBAAgB,MAAO,2BAA2B;AACzD,SAASC,aAAY,QAAS,eAAe;AAC7C,SAASC,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAeP,eAAe,CAAC;EAC7BQ,UAAU,EAAE;IACVP;EACF,CAAC;EACDQ,QAAQ,EAAE;IACR,GAAGH,UAAU,CAAC,CAAC,QAAQ,CAAC,CAAC,CAAE;EAC7B,CAAC;EACDI,OAAO,EAAE;IACP,GAAGH,UAAU,CAAC,CAAC,UAAU,EAAE,aAAa,CAAC,CAAC;IAC1CI,gBAAgBA,CAACC,UAAU,EAAE;MAC3B,MAAMC,KAAI,GAAIC,MAAM,CAAC,gBAAgB,CAAC;MACtC,IAAID,KAAK,EAAE;QACT,MAAME,KAAI,GAAI;UACZC,EAAE,EAAEX,aAAa,CAAC,CAAC;UACnBQ,KAAK;UACLI,KAAK,EAAEL,UAAU,CAACM,QAAQ;UAC1BC,GAAG,EAAEP,UAAU,CAACQ,MAAM;UACtBC,MAAM,EAAET,UAAU,CAACS;QACrB,CAAC;QACD,IAAI,CAACC,QAAQ,CAACP,KAAK,CAAC;MACtB;IACF,CAAC;IACDQ,gBAAgBA,CAACC,SAAS,EAAE;MAC1B,IAAIC,OAAO,CAAE,6BAA4BD,SAAS,CAACT,KAAK,CAACF,KAAM,GAAE,CAAC,EAAE;QAClE,IAAI,CAACa,WAAW,CAACF,SAAS,CAACT,KAAK,CAACC,EAAE,CAAC;MAC