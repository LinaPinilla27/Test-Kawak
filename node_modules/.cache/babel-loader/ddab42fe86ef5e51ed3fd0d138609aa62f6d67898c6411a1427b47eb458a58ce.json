{"ast":null,"code":"import 'bootstrap/dist/css/bootstrap.css';\nimport { defineComponent } from 'vue';\nimport { mapGetters, mapActions } from 'vuex';\nexport default defineComponent({\n  computed: {\n    ...mapGetters(['events']),\n    filteredEvents() {\n      // Filtrar por nombre y fecha\n      return this.events.filter(event => event.title.toLowerCase().includes(this.nameFilter.toLowerCase()) && this.dateFilter ? event.start.includes(this.dateFilter) : true);\n    }\n  },\n  data() {\n    return {\n      nameFilter: '',\n      // Almacena el valor del filtro por nombre\n      dateFilter: '' // Almacena el valor del filtro por fecha\n    };\n  },\n  methods: {\n    ...mapActions(['removeEvent'])\n  }\n});","map":{"version":3,"names":["defineComponent","mapGetters","mapActions","computed","filteredEvents","events","filter","event","title","toLowerCase","includes","nameFilter","dateFilter","start","data","methods"],"sources":["/Users/lina/Desktop/vue/notes-calendar/src/components/ListaNotas.vue"],"sourcesContent":["<template>\n  <div>\n    <label for=\"filterName\">Filtrar por nombre:</label>\n    <input type=\"text\" v-model=\"nameFilter\" id=\"filterName\" />\n    <br />\n    <label for=\"filterDate\">Filtrar por fecha:</label>\n    <input type=\"date\" v-model=\"dateFilter\" id=\"filterDate\" />\n    <br />\n    <template v-if=\"filteredEvents.length > 0\">\n      <table class=\"table\">\n        <thead>\n          <tr>\n            <th>ID</th>\n            <th>Title</th>\n            <th>Start</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(event, index) in filteredEvents\" :key=\"index\">\n            <td>{{ event.id }}</td>\n            <td>{{ event.title }}</td>\n            <td>{{ event.start }}</td>\n          </tr>\n        </tbody>\n      </table>\n    </template>\n    <template v-else>\n      <p>No hay notas disponibles.</p>\n    </template>\n  </div>\n</template>\n\n<script>\nimport 'bootstrap/dist/css/bootstrap.css';\n\nimport { defineComponent } from 'vue';\nimport { mapGetters, mapActions } from 'vuex';\n\nexport default defineComponent({\n  computed: {\n    ...mapGetters(['events']),\n    filteredEvents() {\n      // Filtrar por nombre y fecha\n      return this.events.filter(event =>\n        event.title.toLowerCase().includes(this.nameFilter.toLowerCase()) &&\n        this.dateFilter ? event.start.includes(this.dateFilter) : true\n      );\n    },\n  },\n  data() {\n    return {\n      nameFilter: '',   // Almacena el valor del filtro por nombre\n      dateFilter: '',   // Almacena el valor del filtro por fecha\n    };\n  },\n  methods: {\n    ...mapActions(['removeEvent']),\n  },\n});\n</script>"],"mappings":"AAiCA,OAAO,kCAAkC;AAEzC,SAASA,eAAc,QAAS,KAAK;AACrC,SAASC,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAeF,eAAe,CAAC;EAC7BG,QAAQ,EAAE;IACR,GAAGF,UAAU,CAAC,CAAC,QAAQ,CAAC,CAAC;IACzBG,cAAcA,CAAA,EAAG;MACf;MACA,OAAO,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,KAAI,IAC5BA,KAAK,CAACC,KAAK,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACC,UAAU,CAACF,WAAW,CAAC,CAAC,KAChE,IAAI,CAACG,UAAS,GAAIL,KAAK,CAACM,KAAK,CAACH,QAAQ,CAAC,IAAI,CAACE,UAAU,IAAI,IAC5D,CAAC;IACH;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLH,UAAU,EAAE,EAAE;MAAI;MAClBC,UAAU,EAAE,EAAE,CAAI;IACpB,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACP,GAAGb,UAAU,CAAC,CAAC,aAAa,CAAC;EAC/B;AACF,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}